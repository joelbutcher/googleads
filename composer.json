{
    "name": "joelbutcher/googleads",
    "description": "A lightweight and easy-to-configure extension for the Google Ads PHP Client, with OAuth2 support.",
    "keywords": ["googleads", "google", "advertising"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/joelbutcher/googleads",
        "source": "https://github.com/joelbutcher/googleadss"
    },
    "authors": [
        {
            "name": "Joel Butcher",
            "email": "joel@joelbutcher.co.uk"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "googleads/google-ads-php": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JoelButcher\\GoogleAds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}